# Core
GEMINI_API_KEY=your_gemini_api_key_here
PORT=4000
FRONTEND_URL=https://clipmil.com
CLIPMIL_DOMAIN=api.clipmil.com
SESSION_SECRET=replace_me

# Database
DATABASE_URL=postgresql://user:pass@host:5432/dbname

# Redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD=replace_me

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_or_service_key

# Google Cloud Storage (preferred)
GCS_PROJECT_ID=your_gcp_project_id
GCS_BUCKET_NAME=your_bucket_name
GCS_CLIENT_EMAIL=your_service_account_email
GCS_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----
GCS_CDN_URL=

# YouTube OAuth
YOUTUBE_CLIENT_ID=your_client_id.apps.googleusercontent.com
YOUTUBE_CLIENT_SECRET=your_client_secret
YOUTUBE_REDIRECT_URIS=http://localhost:4000/auth/youtube/callback
YOUTUBE_JAVASCRIPT_ORIGINS=http://localhost:4000
YOUTUBE_TOKENS_JSON=

# AWS S3 (optional alternative to GCS)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-bucket-name
CLOUDFRONT_URL=

# ElevenLabs
ELEVENLABS_API_KEY=your_elevenlabs_api_key
